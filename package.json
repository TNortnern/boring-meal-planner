{
  "name": "boring-meal-planner",
  "version": "0.0.1",
  "private": true,
  "description": "BORING Meal Planner - Simple, effective nutrition planning",
  "scripts": {
    "dev": "docker compose up",
    "dev:nuxt": "pnpm --filter nuxt dev",
    "dev:payload": "pnpm --filter payload dev",
    "build": "pnpm -r build",
    "build:nuxt": "pnpm --filter nuxt build",
    "build:payload": "pnpm --filter payload build",
    "test": "pnpm -r test",
    "test:nuxt": "pnpm --filter nuxt test",
    "test:payload": "pnpm --filter payload test",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "overrides": {
      "vue": "^3.5.25",
      "@vue/runtime-core": "^3.5.25",
      "@vue/server-renderer": "^3.5.25",
      "@vue/compiler-sfc": "^3.5.25"
    }
  }
}
